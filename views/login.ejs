<!DOCTYPE html>
<html>

<head>
  <title>
    <%= title %>
  </title>
  <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700" rel="stylesheet">
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <% include ./partials/header %>
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body class="purple valign-wrapper">
  <div class="container">
    <!-- Page Content goes here -->
    <div class="row">
      <div class="col s12 m6 offset-m3">
        <div class="card">
          <div class="card-content grey-text">
            <span class="card-title">Sign In</span>
            <div class="row">

              <div class="input-field col s12">
                <i class="material-icons prefix">account_circle</i>
                <input id="icon_prefix" type="text" class="validate">
                <label for="icon_prefix">Username</label>
              </div>
              <div class="input-field col s12">
                <i class="material-icons prefix">account_circle</i>
                <input id="icon_prefix" type="text" class="validate">
                <label for="icon_prefix">Password</label>
              </div>
              <div class="input-field col s12">
                  <a class="waves-effect btn rounded caribbean-green"><i class="material-icons left">cloud</i>Sign In</a>
                  <p class="center"><a href="">Forgot Password ?</a></p>
              </div>
            </div>
          </div>
          <div class="card-content grey-text">
            <p class="center">Don't have an account ?, <a class="" href="javascript:void(0);">Sign Up</a> </p>
          </div>
        </div>
      </div>
    </div>
  </div>

  <% include ./partials/footer %>
</body>

</html>